<binding xmlns:tns="http://org.openengsb/connector/mvn/messages" name="binding"
  package="org.openengsb.maven.common.messages">
  <namespace uri="http://org.openengsb/connector/mvn/messages" default="elements" />
  <mapping class="org.openengsb.messages.maven.InstallFileMessage" name="installfilemessage">
    <structure name="filedescriptor" field="fileDescriptor">
      <value name="filepath" field="filePath" />
      <value name="groupid" field="groupId" />
      <value name="artifactid" field="artifactId" />
      <value name="version" field="version" />
      <value name="packaging" field="packaging" />
    </structure>
  </mapping>
</binding>